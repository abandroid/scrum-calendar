{
    "name": "endroid/calendar-scrum",
    "description": "Calendar Scrum",
    "keywords": ["endroid", "calendar", "scrum", "bundle", "symfony"],
    "homepage": "https://github.com/endroid/CalendarScrum",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "endroid/calendar": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\CalendarScrum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\CalendarScrum\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
