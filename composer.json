{
    "name": "endroid/calendar-scrum-bundle",
    "description": "Endroid Calendar Scrum Bundle",
    "keywords": ["endroid", "calendar", "scrum", "bundle", "symfony"],
    "homepage": "https://github.com/endroid/EndroidCalendarScrumBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Bundle\\CalendarScrumBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Bundle\\CalendarScrumBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
